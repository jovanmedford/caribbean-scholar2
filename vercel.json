{
  "name": "caribbean-scholar",
  "version": 2,
  "project": "caribbean-scholar",
  "target": "serverless",
  "builds": [
    {
      "src": "apps/caribbean-scholar",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/caribbean-scholar/$1"
    }
  ]
}
